{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "lint": "next lint",
    "format": "prettier --write **/*.tsx",
    "build": "next build",
    "start": "next start",
    "test": "jest --coverage",
    "test:reset": "jest --updateSnapshot",
    "test:ci": "jest --ci",
    "nyc:coverage": "nyc report --reporter=text-summary --check-coverage",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless",
    "e2e:headless:edge": "start-server-and-test start http://localhost:3000 cypress:headless --browser edge",
    "e2e:headless:chrome": "start-server-and-test start http://localhost:3000 cypress:headless --browser chrome",
    "e2e:headless:firefox": "start-server-and-test start http://localhost:3000 cypress:headless --browser firefox",
    "all": "npm run format && npm run lint && npm run test && npm run build"
  },
  "dependencies": {
    "babel-plugin-istanbul": "^6.1.1",
    "next": "^12.0.10",
    "next-themes": "^0.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@tailwindcss/typography": "^0.5.1",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/react": "17.0.39",
    "autoprefixer": "^10.4.2",
    "babel-jest": "27.4.6",
    "check-code-coverage": "1.10.0",
    "cypress": "^9.4.1",
    "eslint": "8.8.0",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-testing-library": "5.0.5",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "jest": "27.4.7",
    "lodash": ">=4.17.21",
    "postcss": "^8.4.6",
    "prettier": "2.5.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.18",
    "typescript": "4.5.5"
  }
}
